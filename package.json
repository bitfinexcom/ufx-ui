    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "focus-trap-react": "^7.0.1",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "i18next": "^19.5.1",
    "jest-enzyme": "^7.1.2",
    "lodash": "^4.17.15",
    "markdown-to-jsx": "^7.1.2",
    "memoize-one": "^5.1.1",
    "node-sass-chokidar": "^1.5.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "rc-slider": "^9.6.4",
    "react-beforeunload": "^2.2.4",
    "react-datepicker": "^3.3.0",
    "react-grid-layout": "^1.1.1",
    "react-i18next": "^11.7.0",
