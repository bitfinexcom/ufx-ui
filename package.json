{
  "name": "ufx-ui",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "exchange",
    "modular-ui",
    "front-end"
  ],
  "author": "Bitfinex",
  "repository": "https://github.com/bitfinexcom/ufx-ui",
  "main": "dist/ufx-ui.cjs.js",
  "module": "dist/ufx-ui.esm.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "bundle:sass": "scss-bundle -e src/ufx-ui.scss -o src/ufx-ui.bundle.scss -p ./",
    "build:sass": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ufx-ui.bundle.scss src/ufx-ui.css",
    "prefix:css": "postcss --use autoprefixer --use postcss-discard-comments -o dist/css/ufx-ui.css src/ufx-ui.css",
    "compress:css": "node-sass-chokidar --output-style compressed dist/css/ufx-ui.css dist/css/ufx-ui.min.css",
    "build:css": "run-s bundle:sass build:sass prefix:css compress:css",
    "watch:sass": "npm run build:sass && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ufx-ui.scss src/ufx-ui.css --watch --recursive",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "run-s build build:css",
    "test": "run-s test:watch lint test:build",
    "test:build": "run-s build",
    "test:watch": "cross-env react-scripts test --env=jsdom",
    "test:snapshots": "cross-env react-scripts test storyshots.test --env=jsdom",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "install:peer": "install-peers -f",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "deploy:storybook": "storybook-to-ghpages --remote=upstream",
    "docz:dev": "docz dev",
    "docz:clean": "rimraf docz-build",
    "docz:build": "npm run build:sass && npm run docz:clean && docz build",
    "docz:serve": "docz build && docz serve",
    "precommit-echo": "echo 'Running git pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-echo",
    "lint"
  ],
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.10.1",
    "@mdx-js/mdx": "^1.6.16",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.21",
    "@storybook/storybook-deployer": "^2.8.7",
    "@svgr/rollup": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.1.1",
    "docz": "^2.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-plugin-svgr": "^2.0.2",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.1.0",
    "jest-canvas-mock": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-discard-comments": "^4.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "react-scripts": "^3.4.1",
