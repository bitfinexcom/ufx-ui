{
  "name": "ufx-ui",
  "version": "0.2.5",
  "description": "A UI toolkit to build trading interface",
  "keywords": [
    "exchange",
    "trading-ui",
    "microfinex-ui",
    "front-end"
  ],
  "author": "Bitfinex",
  "license": "Apache-2.0",
  "repository": "https://github.com/bitfinexcom/ufx-ui",
  "main": "dist/ufx-ui.cjs.js",
  "module": "dist/ufx-ui.esm.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "run-s build build:css",
    "cy:test": "cd ./packages/cypress-tests && npm run cy:run",
    "example:build": "cd ./example && npm run build",
    "verify": "run-s lint prepare example:build build:storybook test:ufx-ui",
    "test:snapshots": "cross-env react-scripts test storyshots.test --watchAll=false",
    "test:snapshots:watch": "cross-env react-scripts test storyshots.test",
    "test:unit": "cross-env react-scripts test --watchAll=false",
    "test:unit:watch": "cross-env react-scripts test",
    "test:unit:ready": "cross-env react-scripts test src/components/ui src/components/format src/components/Book src/components/OrderHistory --watchAll=false",
    "test:unit:ready:watch": "cross-env react-scripts test src/components/ui src/components/format src/components/Book src/components/OrderHistory",
    "test:ufx-ui": "run-s test:snapshots test:unit:ready",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "install:peer": "install-peers -f"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
